<form method="" class="modal fade" id="modalEditUser" role="dialog" aria-labelledby="dataModalCenterTitle"
      aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLongTitle">Modifier un utilisateur</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body modal-input container-fluid"></div>

            <div class="error-msg p-4"></div>
            <div class="modal-footer">
                <div class="btn" data-dismiss="modal">Annuler</div>
                <button id="submitEditUser" type="button" class="btn btn-primary data"
                        onclick="initialiserModalUser(Routing.generate('edit_user',true), $('#modalEditUser'))">
                    Enregistrer
                </button>
            </div>
        </div>
    </div>
</form>